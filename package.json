{
  "name": "dugit2",
  "description": "GitHub Classroom tools for Drury University",
  "version": "1.0.0",
  "author": "Brayden O'Neal",
  "homepage": "https://github.com/braydenoneal/dugit",
  "repository": "braydenoneal/dugit",
  "bugs": "https://github.com/braydenoneal/dugit/issues",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "dugit2": "dist/index.js"
  },
  "scripts": {
    "build": "esbuild --format=cjs --target=node22 --platform=node --bundle --outfile=dist\\dugit2.cjs src\\index.ts",
    "sea-prep": "node --experimental-sea-config sea-config.json",
    "copy-node": "node -e \"require('fs').copyFileSync(process.execPath, 'dist\\\\dugit2.exe')\"",
    "inject": "npx postject dist\\dugit2.exe NODE_SEA_BLOB dist\\sea-prep.blob --sentinel-fuse NODE_SEA_FUSE_fce680ab2cc467b6e072b8b5df1996b2",
    "start": "dist\\dugit2.exe",
    "dev": "tsx src/index.ts"
  },
  "devDependencies": {
    "@types/slug": "^5.0.9",
    "esbuild": "^0.24.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.2.3",
    "axios": "^1.7.9",
    "chalk": "^5.4.1",
    "octokit": "^4.1.0",
    "open": "^10.1.0",
    "ora": "^8.1.1",
    "simple-git": "^3.27.0",
    "slug": "^10.0.0",
    "unique-names-generator": "^4.7.1"
  }
}
